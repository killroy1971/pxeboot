MENU TITLE OS Install Menu

LABEL Centos7
MENU LABEL Install Centos 7.2
kernel centos7/isolinux/vmlinuz
append initrd=centos7/isolinux/initrd.img inst.repo=http://192.168.4.11:20000/centos7

LABEL Centos6
MENU LABEL Install Centos 6.8
kernel centos6/isolinux/vmlinuz
append initrd=centos6/isolinux/initrd.img inst.repo=http://192.168.4.11:20000/centos6

LABEL Fedora-WKS-Install
MENU LABEL Fedora 25 Workstation Install
kernel Fedora-Workstation/images/pxeboot/vmlinuz
append initrd=Fedora-Workstation/images/pxeboot/initrd.img ro root=live:http://192.168.4.11:20000/Fedora-Workstation/LiveOS/squashfs.img rd.live.image inst.stage2=http://192.168.4.11:20000/Fedora-Workstation ip=dhcp

LABEL Fedora-SVR-Install
MENU LABEL Fedora 25 Server Install
kernel Fedora-Server/images/pxeboot/vmlinuz
append initrd=Fedora-Server/images/pxeboot/initrd.img inst.repo=http://192.168.4.11:20000/Fedora-Server inst.stage2=http://192.168.4.11:20000/Fedora-Server ip=dhcp

LABEL Return
MENU LABEL Return
kernel pxelinux.cfg/vesamenu.c32
append pxelinux.cfg/default

