MENU TITLE Tools Menu

LABEL ESET
MENU LABEL Run ESET SysRescue
kernel images/eset/vmlinuz
append root=/dev/nfs boot=casper initrd=images/eset/initrd.lz netboot=nfs nfsroot=192.168.4.10:/srv/netboot/eset-sysrescue

LABEL centos6-rescue
MENU LABEL CentOS 6.8 Rescue
kernel images/centos6/vmlinuz
append initrd=images/centos6/initrd.img inst.repo=http://192.168.4.10/pub/os/centos6 rescue

LABEL centos7-rescue
MENU LABEL CentOS 7.2 Rescue
kernel images/centos7/vmlinuz
append initrd=images/centos7/initrd.img inst.repo=http://192.168.4.10/pub/os/centos7 rescue

LABEL SpinRite
MENU LABEL SpinRite 6
kernel images/bootdisk/memdisk
append initrd=images/bootdisk/spinrite.img floppy raw

LABEL Memtest
MENU LABEL Memtest
kernel images/bootdisk/memtestp0
append -

LABEL Return
MENU LABEL Return
kernel vesamenu.c32
append pxelinux.cfg/default
