default pxelinux.cfg/vesamenu.c32
prompt 0

MENU TITLE PXE Boot Menu

LABEL Kali
MENU LABEL Kali Linux Live (amd64 failsafe)
linux /kali/vmlinuz
append root=/dev/nfs initrd=/kali/initrd.img boot=live netboot=nfs nfsroot=192.168.4.11:/netboot/Kali/ nomodeset username=root hostname=kali nosplash

LABEL Mint18
MENU LABEL Linux Mint XFCE 18 Live DVD
kernel mint/casper/vmlinuz
append root=/dev/nfs initrd=mint/casper/initrd.lz boot=casper netboot=nfs nfsroot=192.168.4.11:/netboot/Mint 

LABEL Fedora-WKS-Live
MENU LABEL Fedora 24 Workstation Live
kernel Fedora-Workstation/images/pxeboot/vmlinuz
append initrd=Fedora-Workstation/images/pxeboot/initrd.img ro root=live:http://192.168.4.11:20000/Fedora-Workstation/LiveOS/squashfs.img rd.live.image 

LABEL OS Install
MENU LABEL OS-Install
kernel pxelinux.cfg/vesamenu.c32
append pxelinux.cfg/osinstall

LABEL Tools
MENU LABEL Tools
kernel pxelinux.cfg/vesamenu.c32
append pxelinux.cfg/tools

LABEL OS Kickstart
MENU LABEL OS-Kickstart
kernel pxelinux.cfg/vesamenu.c32
append pxelinux.cfg/oskickstart
